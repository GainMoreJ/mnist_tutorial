<script type="text/javascript">
var casenum = '1';
var sw_mode = '1';
var wlc_psta = '';
var wlc_express = '0';
if( ((sw_mode == 2 || sw_mode == 3) && wlc_psta == 1) || (sw_mode == 3 && wlc_psta == 3))
sw_mode = 4;
if(sw_mode == 3 && wlc_psta == 2)
sw_mode = 2;
var wanstate = -1;
var wansbstate = -1;
var wanauxstate = -1;
wanstate = 2;
wansbstate = 0;
wanauxstate = 1;

var isSupport = function(_ptn){return ('mssid 2.4G update rawifi switchctrl manual_stb pwrctrl ipv6 PARENTAL2 loclist pptpd HTTPS vpnc repeater sfp user_low_rssi tcode realip reboot_schedule'.search(_ptn) == -1) ? false : true;}
var tmo_support = isSupport("tmo");
var dsl_support = isSupport("dsl");
var wans_dualwan_orig = '';
var wans_flag = (wans_dualwan_orig.search("none") == -1) ? 1 : 0;
if(wans_dualwan_orig.search(" ") == -1)
wans_flag = 0;
var new_lan_ip = [{next_lanip: '192.168.1.1'}][0].next_lanip;
function initial(){
function is_logined() { return 0; }
function login_ip_dec() { return '0'; }
function login_ip_str() { return '0.0.0.0'; }
function login_ip_str_now() { return '192.168.1.188'; }
function login_mac_str() { return '14:ab:c5:ec:dd:58'; }

var LoginIP = login_ip_str();
var nowIP = login_ip_str_now();
var html_code = '';
var url_flag = "";
var html_code_notlogin = "<ul><li>Please check with your network administrator.</li></ul>";
var showtext = function(obj, str){if(obj)obj.innerHTML=str;}
if(casenum == 1){
$("#page_title").html("未连接网线。");
html_code += "<ul>";
html_code += "<li>请确认 RT-N12+ 的 WAN 端口与互联网之间的网路线已接上。</li>";
html_code += "<li>请确认是否需要更改 <a href='/Advanced_OperationMode_Content.asp'>操作模式。</a></li>";
html_code += "</ul>";
$("#wanLink").click(function(){
detect_general();
});
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 2){
html_code += "<ul>";
html_code += "<li>";
if(wansbstate == 2 || wanauxstate == 3){ //Authentication Failed
$("#page_title").html("PPPoE 认证失败。");
html_code += "<div>您的 PPPoE 或是 VPN 的帐号、密码不被接受，请 <a href='/Advanced_WAN_Content.asp'>重新输入。</a></div>";
$("#wanLink").click(function(){
});
}
else if(wansbstate == 1){ //No Response
}
else if(wansbstate == 10){ //Terminal due to lack of activity
}
else{ //Connecting
$("#animation_field").show();
detect_ppp_connect();
$("#page_title").html("Please Wait for Internet Reconnecting");
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggest_title").hide();
$("#suggestion").hide();
$("#case_content").hide();
$("#back_btn").show();
$("#wanLink").click(function(){
history.go(-1);
});
$("#wanLink").html("上一页");
}
html_code += "</li>";
html_code += "</ul>";
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 3){
$("#page_title").html("无法连接到互联网");
html_code += "<ul>";
html_code += "<li>请确认网路设置内容正确。</li>";
html_code += "<li>请与您的 ISP ( 网络服务供货商 ) 联系，以取得 DHCP 错误的原因。</li>";
html_code += "</ul>";
$("#wanLink").click(function(){
detect_general();
});
$("#wanLink").html("切换WAN类型");
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 4){
showtext($("#failReason1")[0], "互联网连接失败。路由器的 IP 地址与网关的 IP 地址相同。");
html_code += "<ul>";
html_code += "<li>请再次设置路由器的 IP 地址，路由器 IP 地址需与网关 IP 地址不同。</li>";
html_code += "</ul>";
$("#wanLink").click(function(){
detect_general();
});
}
else if(casenum == 5){
if(wanstate == 5){
$("#page_title").html("您已手动中断WAN连线。");
}
else{
$("#page_title").html("USB网卡正在重开机或关闭");
}
html_code += '<ul>';
if(wanstate == 5){
html_code += "<li>建议开启WAN连线。 <a href='/index.asp'>更改设置。</a></li>";
}
else{
html_code += "<li>请检查USB网卡是否已正常开机完成。</li>";
html_code += "<li>更改您的<a href='/Advanced_Modem_Content.asp'>USB网卡设置。</a></li>";
}
html_code += '</ul>';
if(wanstate == 5){
$("#wanLink").click(function(){
manually_start_wan_Link();
});
}else{
$("#wanLink").click(function(){
detect_general();
});
}
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum.search("6") !== -1){
$("#page_title").html("检测到 IP 地址冲突。");
html_code += "<ul>";
html_code += "<li>点击【下一步】更换成新的 IP 地址。新的 IP 地址为：";
html_code += "<span style='font-weight:bolder'>"+ new_lan_ip +"</span></li>";
html_code += "<li>更改IP地址后，请确认相关服务的IP设置是否需要调整，如端口转发或 DMZ 等功能。</li>";
html_code += "</ul>";
$("#wanLink").html("下一步");
$("#wanLink").click(function(){
change_lan_subnet();
});
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 8){
$("#page_title").html("无法与Parent-AP 连线。");
if(LoginIP != "0.0.0.0" && LoginIP != nowIP){
html_code = html_code_notlogin;
$("#wanLink").hide();
}
else{
html_code += "<ul>";
html_code += "<li>请确认前端基地台的网路连线正常。</li>";
html_code += "<li><a style='color:#FFF' href='/QIS_wizard.htm?flag=manual'>重新设置连线</a>。</li>";
html_code += "</ul>";
if(sw_mode == "2"){
if(wlc_express == '1')
url_flag += "sitesurvey_exp2";
else if(wlc_express == '2')
url_flag += "sitesurvey_exp5";
else
url_flag += "sitesurvey_rep";
}
else
url_flag += "sitesurvey_mb";
$("#wanLink").click(function(){
top.location.href = "/QIS_wizard.htm?flag=" + url_flag;
});
}
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else{
parent.location = "/index.asp";
}
$("#case_content")[0].innerHTML = html_code;
if(casenum == "6_1"){
change_lan_subnet();
}
var windowHeight = (function(){
if(window.innerHeight)
return window.innerHeight;
else if(document.body && document.body.clientHeight)
return document.body.clientHeight;
else if(document.documentElement && document.documentElement.clientHeight)
return document.documentElement.clientHeight;
else
return 800;
})();
document.getElementById("loginTable").style.display = "";
}
function pppoeLink(){
top.location.href = "/QIS_wizard.htm?flag=pppoe";
}
function detectLink(){
top.location.href = "/QIS_wizard.htm?flag=detect";
}
function manually_start_wan_Link(){
top.location.href = "/index.asp?flag=Internet";
}
function change_lan_subnet(){
top.location.href = "/updateSubnet.htm";
}
function iAmAlive(json){
top.location.href = location.origin.replace(location.hostname, new_lan_ip)
+ "/QIS_wizard.htm?flag="
+ (function(){return (('1' === '0')?"welcome":"detect")})();
}
function detect_router(){
$.ajax({
url: location.origin.replace(location.hostname, new_lan_ip) + "/httpd_check.json",
dataType: 'jsonp',
timeout: 2000,
error: function(xhr){
setTimeout(detect_router, 1000);
setTimeout(function(){
$("#case_content ul").html("<li>IP 地址被更新。若此步骤花费了超过两分钟的时间，请按照以下三个步骤手动更新 IP 地址：（1）拔掉网线； （2）等待约十秒钟；（3）重新插上网线以获取一个新的 IP 地址。</li>");
}, 3000);
}
});
}
function detect_general(){
if(wans_flag){ //Dual WAN
if(wans_dualwan_orig.split(" ")[0].toUpperCase == "USB"){
top.location.href = '/Advanced_Modem_Content.asp';
}else if(dsl_support){
if(wans_dualwan_orig.split(" ")[0].toUpperCase == "WAN" || wans_dualwan_orig.split(" ")[0].toUpperCase == "LAN")
top.location.href = '/Advanced_WAN_Content.asp';
else
top.location.href = '/Advanced_DSL_Content.asp';
}
else
top.location.href = '/Advanced_WAN_Content.asp';
}
else{ //Single WAN
if(dsl_support)
top.location.href = '/Advanced_DSL_Content.asp';
else if(sw_mode == 3)
top.location.href = '/index.asp';
else
top.location.href = '/Advanced_WAN_Content.asp';
}
}
function detect_ppp_connect(){
$.ajax({
url: '/WAN_info.asp',
dataType: 'script',
error: function(xhr){
setTimeout(detect_ppp_connect, 1000);
},
success: function(response){
if( wanstate == "2" && wansbstate == "0" && wanauxstate == "0"){
top.location.href = document.referrer;
}
else{
setTimeout(detect_ppp_connect, 1000);
}
}
});
}
</script>
